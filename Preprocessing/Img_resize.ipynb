   "source": [
    "from PIL import Image\n",
    "import os\n",
    "\n",
    "dir = './데드리프트/'\n",
    "imgs = os.listdir(dir)\n",
    "\n",
    "for img in imgs:\n",
    "    try:\n",
    "        source_image = (dir+img)\n",
    "        image = Image.open(source_image)\n",
    "        resize_image = image.resize((416,416))\n",
    "        resize_image.save(source_image)\n",
    "    except:\n",
    "        pass"
   ]
